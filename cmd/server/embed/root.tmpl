<html>
<head>
  <title>Benchmarks</title>
  <link rel="stylesheet" href="/style.css">
</head>

<body>
  <table class="root">
    <tr><td>
      <fieldset>
        <legend>Tests</legend>
        <table>
          {{ range .Data.TestTags }}
            <tr><td><a href="test/{{ . }}.html">{{ $.Data.TestName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td><td>
      <fieldset>
        <legend>Handlers</legend>
        <table>
          {{ range .Data.HandlerTags }}
            <tr><td><a href="handler/{{ . }}.html">{{ $.Data.HandlerName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td></tr>
    {{ if .Data.HasWarningText }}
      <tr><td colspan=2 align="left" class="warnings"><pre>
{{range $val := .Data.WarningText }}{{ $val }}
{{end}}</pre></td></tr>
    {{ end }}
  </table>
</body>
</html>
