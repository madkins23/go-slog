<html>
<head>
  <title>Benchmarks</title>
  <link rel="stylesheet" href="/style.css">
</head>

<body>
  <table class="root">
    <tr><td>
      <fieldset>
        <legend>Tests</legend>
        <table>
          {{ range .Data.TestTags }}
            <tr><td><a href="test?tag={{ . }}">{{ $.Data.TestName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td><td>
      <fieldset>
        <legend>Handlers</legend>
        <table>
          {{ range .Data.HandlerTags }}
            <tr><td><a href="handler?tag={{ . }}">{{ $.Data.HandlerName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td></tr>
  </table>
</body>
</html>
