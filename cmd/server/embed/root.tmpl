<html>
<head>
  <title>Benchmark</title>
  <link rel="stylesheet" href="/style.css">
</head>

<body>
<div class="top">
  <table class="root">
    <tr><td>
      <fieldset>
        <legend>Benchmarks</legend>
        <table>
          {{ range .Data.BenchTags }}
            <tr><td><a href="bench?tag={{ . }}">{{ $.Data.BenchName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td><td>
      <fieldset>
        <legend>Handlers</legend>
        <table>
          {{ range .Data.HandlerTags }}
            <tr><td><a href="handler?tag={{ . }}">{{ $.Data.HandlerName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td></tr>
  </table>
</div>
</body>
</html>
