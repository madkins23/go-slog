<html>
<head>
  <title>Benchmarks</title>
  <link rel="stylesheet" href="/go-slog/style.css">
</head>

<body>
  <table class="root">
    <tr><td>
      <fieldset>
        <legend>Tests</legend>
        <table>
          {{ range .Bench.TestTags }}
            <tr><td><a href="/go-slog/test/{{ . }}.html">{{ $.Bench.TestName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td><td>
      <fieldset>
        <legend>Handlers</legend>
        <table>
          {{ range .Bench.HandlerTags }}
            <tr><td><a href="/go-slog/handler/{{ . }}.html">{{ $.Bench.HandlerName . }}</a></td></tr>
          {{ end }}
        </table>
      </fieldset>
    </td></tr>
    {{ if .Bench.HasWarningText }}
      <tr><td colspan=2 align="left" class="warnings"><pre>
{{range $val := .Bench.WarningText }}{{ $val }}
{{end}}</pre></td></tr>
    {{ end }}
  </table>
</body>
</html>
