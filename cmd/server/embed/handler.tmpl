<html>
<head>
  <title>Benchmark Data</title>
  <link rel="stylesheet" href="/style.css">
</head>

<body>
<table class="top">
  <tr>
    <td>
      <h1>Handler {{ .Data.HandlerName .Handler }}</h1>
    </td>
    <td align="right">
      {{ template "choices" $ }}
    </td>
  </tr>
  <tr>
    <td colspan=2 align="center">
      <table class="data">
        <tr>
          <th>Benchmark</th>
          <th>Runs</th>
          <th>Ns/Op</th>
          <th>Allocs/Op</th>
          <th>Bytes/Op</th>
          <th>GB/Sec</th>
        </tr>
        {{ range $tag, $record := .Data.TestRecords .Handler }}
          <tr>
            <td>{{ $.Data.TestName $tag }}</td>
            <td class="number">{{ $.FixInt $record.Iterations }}</td>
            <td class="number">{{ $.FixFloat $record.NanosPerOp }}</td>
            <td class="number">{{ $.FixInt $record.MemAllocsPerOp }}</td>
            <td class="number">{{ $.FixInt $record.MemBytesPerOp }}</td>
            <td class="number">{{ $.FixFloat $record.GbPerSec }}</td>
          </tr>
        {{ end }}
      </table>
    </td>
  </tr>
  <tr>
    <td colspan=2>
      <table>
        <tr>
          <td><img src="chart.svg?tag={{ .Handler }}&item=Nanos" alt="{{ .Data.HandlerName .Handler }} Ns/Op" /></td>
          <td><img src="chart.svg?tag={{ .Handler }}&item=MemAllocs" alt="{{ .Data.HandlerName .Handler }} Allocs/Op" /></td>
        </tr>
        <tr>
          <td><img src="chart.svg?tag={{ .Handler }}&item=MemBytes" alt="{{ .Data.HandlerName .Handler }} Bytes/Op" /></td>
          <td><img src="chart.svg?tag={{ .Handler }}&item=MemMB" alt="{{ .Data.HandlerName .Handler }} MB/Sec" /></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
</div>
</body>
</html>
